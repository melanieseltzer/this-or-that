{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-react-a11y",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "react-anchor-blank-noopener": true,
    "no-console": false,
    "member-access": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false
  }
}